<?xml version="1.0"?>
<extension key="superset" type="module">
  <file>superset</file>
  <name>superset</name>
  <description>Integrate Superset dashboards into CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Manuel Flandorfer</author>
    <email>mflandor@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-03-18</releaseDate>
  <version>1.0.0-alpha.2</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Superset</namespace>
    <format>25.01.1</format>
    <angularModule>crmSuperset</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>afform-entity-php@1.0.0</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Superset\AutomaticUpgrader</upgrader>
</extension>
